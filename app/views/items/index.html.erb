<h1>Listing Items</h1>
 
<%= link_to 'New item', new_item_path %>

<table>
  <tr>
    <th>Name</th>
    <th>Date</th>
    <th>Status</th>
    <th colspan="3"></th>
  </tr>
 
  <% @items.each do |item| %>
    <tr>
      <td><%= item.title %></td>
      <td><%= item.link.html_safe %> </td>
      <td><%= link_to 'Show', item_path(item) %></td>
      <td><%= link_to 'Edit', edit_item_path(item) %></td>
      <td><%= link_to 'Destroy', item_path(item), method: :delete, data: { confirm: 'Sure?' } %></td>
    </tr>
  <% end %>

<!-- <iframe width="420" height="315" src="//www.youtube.com/embed/X34JarNjoIU" frameborder="0" allowfullscreen></iframe> -->

</table>