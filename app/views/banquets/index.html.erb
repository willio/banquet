<h1>Listing Banquets</h1>
 
<%= link_to 'New banquet', new_banquet_path %>

<table>
  <tr>
    <th>Name</th>
    <th>Date</th>
    <th>Status</th>
    <th colspan="3"></th>
  </tr>
 
  <% @banquets.each do |banquet| %>
    <tr>
      <td><%= banquet.title %></td>
      <td><%= banquet.link.html_safe %> </td>
      <td><%= link_to 'Show', banquet_path(banquet) %></td>
      <td><%= link_to 'Edit', edit_banquet_path(banquet) %></td>
      <td><%= link_to 'Destroy', banquet_path(banquet), method: :delete, data: { confirm: 'Sure?' } %></td>
    </tr>
  <% end %>

<!-- <iframe width="420" height="315" src="//www.youtube.com/embed/X34JarNjoIU" frameborder="0" allowfullscreen></iframe> -->

</table>